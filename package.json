{
  "name": "medpacker",
  "author": "pipopotamasu",
  "license": "MIT",
  "description": "setup fontend assets in Ruby on Rails for MedPeer, Inc",
  "scripts": {
    "dev": "webpack --mode=development --progress --config webpack.dev.js",
    "dev:server": "webpack-dev-server --progress --color --hot --config webpack.dev.js",
    "dev:watch": "webpack --mode=development --progress --watch --config webpack.dev.js",
    "build": "webpack --mode=production --config webpack.prod.js",
    "eslint": "eslint app/bundles/javascripts --ext js,vue",
    "eslint:fix": "eslint app/bundles/javascripts --ext js,vue --fix",
    "stylelint": "stylelint app/bundles/stylesheets/**/*.scss app/bundles/javascripts/components/**/*.vue",
    "stylelint:fix": "stylelint app/bundles/stylesheets/**/*.scss app/bundles/javascripts/components/**/*.vue --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "rails-ujs": "^5.2.2",
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-vue": "^5.1.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.1",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  }
}
