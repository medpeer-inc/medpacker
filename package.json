    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "autoprefixer": "^9.7.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.4.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
