{
  "name": "rails-webpack-template",
  "private": true,
  "scripts": {
    "dev": "webpack --mode=development --progress --config webpack.dev.js",
    "dev:server": "webpack-dev-server --progress --color --hot --config webpack.dev.js",
    "dev:watch": "webpack --mode=development --progress --watch --config webpack.dev.js",
    "build": "webpack --mode=production --config webpack.prod.js",
    "eslint": "eslint app/bundles/javascripts --ext js",
    "eslint:fix": "eslint app/bundles/javascripts --ext js --fix",
    "stylelint": "stylelint app/bundles/stylesheets/**/*.scss",
    "stylelint:fix": "stylelint app/bundles/stylesheets/**/*.scss --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.4.0",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4"
  }
}
